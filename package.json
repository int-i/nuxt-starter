{
  "name": "nuxt-starter",
  "version": "1.0.0",
  "description": "My Nuxt.js Start Template",
  "keywords": [
    "nuxt",
    "template"
  ],
  "dependencies": {
    "@nuxtjs/axios": "^5.12.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "nuxt": "^2.13.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "push-dir": "^0.4.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . & stylelint **/*.vue",
    "lint:fix": "eslint --ext .js,.vue --fix --ignore-path .gitignore . & stylelint --fix **/*.vue",
    "start": "nuxt start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/int-i/nuxt-starter"
  },
  "author": "Seungjae Park <email@example.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/int-i/nuxt-starter/issues"
  },
  "homepage": "https://github.com/int-i/nuxt-starter#readme",
  "private": true
}
